{
  "name": "covid19-ppe-tracker",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "start": "npm run migrate && npm run seed && node ./bin/www",
    "start:dev": "nodemon ./bin/www | pino-pretty --translateTime='HH:MM:ss'",
    "generateKeys": "node generate-vapid-keys.js",
    "migrate": "npx sequelize db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "~4.16.1",
    "express-pino-logger": "^4.0.0",
    "express-validator": "^6.4.0",
    "morgan": "~1.9.1",
    "nanoid": "^3.0.2",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "web-push": "^3.4.3"
  },
  "engines": {
    "node": "12.x.x"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "pino-pretty": "^4.0.0"
  }
}
